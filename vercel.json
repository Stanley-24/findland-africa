{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://findland-africa.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://findland-africa.onrender.com",
    "REACT_APP_WS_URL": "wss://findland-africa.onrender.com"
  }
}